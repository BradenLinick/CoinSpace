,"C:\\Users\\brade\\Documents\\React-Projects\\coinspace\\src\\components\\AddCoin.jsx",["57"],"import React, { useState, useContext } from 'react'\r\nimport { WatchListContext } from '../context/watchListContext'\r\nimport onClickOutside from 'react-onclickoutside'\r\n\r\nfunction AddCoin() {\r\n\r\n  const [toggleDropdown, setToggleDropdown] = useState(false)\r\n  const { addCoin, watchList } = useContext(WatchListContext)\r\n  AddCoin.handleClickOutside = () => setToggleDropdown(false)\r\n\r\n  const availableCoins = [\r\n    'bitcoin',\r\n    'ethereum',\r\n    'ripple',\r\n    'tether',\r\n    'bitcoin-cash',\r\n    'litecoin',\r\n    'eos',\r\n    'okb',\r\n    'chainlink',\r\n    'tezos',\r\n    'cardano',\r\n    'stellar'\r\n  ]\r\n \r\n  const updated = availableCoins.filter(coin => !watchList.includes(coin))\r\n\r\n  const handleClick = coin => {\r\n    addCoin(coin)\r\n    setToggleDropdown(false)\r\n  }\r\n\r\n  return (\r\n    <div className=\"dropdown\">\r\n      <button \r\n        onClick={() => setToggleDropdown(!toggleDropdown)}\r\n        className=\"btn btn-primary dropdown-toggle\" \r\n        type=\"button\"\r\n      >\r\n        Add Coin\r\n      </button>\r\n      <div className={\r\n        toggleDropdown \r\n        ? 'dropdown-menu show' \r\n        : 'dropdown-menu'\r\n        }\r\n      >\r\n        {updated.map((el, index) => {\r\n          return (\r\n            <a onClick={() => handleClick(el)} key={index} href=\"#\" className=\"dropdown-item\">{el}</a>\r\n          )\r\n        })}\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nconst clickOutsideConfig = {\r\n  handleClickOutside: () => AddCoin.handleClickOutside\r\n}\r\n\r\nexport default onClickOutside(AddCoin, clickOutsideConfig)","C:\\Users\\brade\\Documents\\React-Projects\\coinspace\\src\\index.js",[],["58","59"],"C:\\Users\\brade\\Documents\\React-Projects\\coinspace\\src\\apis\\coinGecko.js",[],"C:\\Users\\brade\\Documents\\React-Projects\\coinspace\\src\\context\\watchLi